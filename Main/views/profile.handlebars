<div class="profilePage">
    <div class="pageTitle">{{ user.name }}'s Profile Page </div>

    <form action="submit" class="eventForm neweventform" id="eventForm">
        <p class="formTitle"> Create an Event!</p>
        <p>Title: <input type="text" name="event-title" id="eventTitleField" placeholder="Enter Event Title..."></p>
        <p>Description: <input type="text" name="event-description" id="eventDescription" placeholder="Enter Event Description..."></p>
        <p>Date: <input type="text" id="datepicker" placeholder="Enter Event Date..."></p>
        <button type="submit">Submit</button>
    </form>
</div>

{{!-- Planning on making your events show up under --}}
<div class="myEventArea">
    {{> event/adminEvent}}
</div>

<script src="https://code.jquery.com/jquery-3.6.0.js"></script>
